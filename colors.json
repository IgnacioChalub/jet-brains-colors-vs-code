 "editor.bracketPairColorization.enabled": true,
    "workbench.colorCustomizations": {
        "editorBracketHighlight.foreground1": "#CFD2D5",
        "editorBracketHighlight.foreground2": "#CFD2D5",
        "editorBracketHighlight.foreground3": "#CFD2D5",
        "editorBracketHighlight.foreground4": "#CFD2D5",
        "editorBracketHighlight.foreground5": "#CFD2D5",
        "editorBracketHighlight.foreground6": "#CFD2D5",
        "editorBracketHighlight.unexpectedBracket.foreground": "#CFD2D5",
    },
    "editor.tokenColorCustomizations": {
        "textMateRules": [
            {
              "name": "entity function name",
              "scope": [
                "entity.name.function",
                "support.class"
                ],
              "settings": {
                "foreground": "#FFC66D"
              }
            },
            {
              "name": "string",
              "scope": "string",
              "settings": {
                "foreground": "#8EA765"
              }
            },
            {
                "name": "storage modifier",
                "scope": [
                    "storage.modifier",
                    "keyword.control",
                    "variable.language",
                    "support.type.primitive.ts",
                    "storage.type.class.ts",
                    "storage.type.ts",
                    "keyword.operator.expression"
                ],
                "settings": {
                  "foreground": "#CC8E2F"
                }
            },
            {
                "name": "variable other property ts",
                "scope": "variable.other.property.ts",
                "settings": {
                  "foreground": "#A782BB"
                }
            },
            {
                "name": "entity name type class",
                "scope": [
                    "entity.name.type.class",
                    "entity.name.type.interface",
                    "variable.parameter",
                    "meta.return.type.ts",
                    "constant.numeric",
                    "variable.other.constant",
                    "variable",
                ],
                "settings": {
                  "foreground": "#CFD2D5"
                }
            },
          ]
        }
